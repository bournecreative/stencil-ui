{
  "name": "stencil-play",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-play/stencil-play.esm.js",
  "homepage": "https://bournecreative.github.io/stencil-ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006 -s ./dist",
    "build-storybook": "storybook build -s ./dist",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "dependencies": {
    "@stencil/core": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/blocks": "^7.4.6",
    "@storybook/html": "^7.4.6",
    "@storybook/html-webpack5": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "gh-pages": "^6.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "21.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.6"
  },
  "license": "MIT"
}
